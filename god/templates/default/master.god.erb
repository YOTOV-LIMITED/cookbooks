God.load "/etc/god/conf.d/*.god"

God::Contacts::Email.defaults do |d|
  d.from_email = 'god@<%= @domain %>'
  d.from_name = 'God'
end

God.contact(:email) do |c|
  c.name = '<%= @email_name %>'
  c.to_email = '<%= @email_name %>@<%= @email_domain %>'
end

